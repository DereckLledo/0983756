        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginBottom="55dp"
        android:layout_marginTop="55dp"
        app:layout_constraintBottom_toTopOf="@+id/textViewGagner"
        app:layout_constraintStart_toStartOf="@+id/spinnerLargeur"
        app:layout_constraintTop_toBottomOf="@+id/spinnerLargeur"
